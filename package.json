{
  "name": "monorepo-apex",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm exec nx run-many --target=build",
    "dev": "pnpm exec nx run-many --target=dev",
    "lint": "pnpm exec nx run-many --target=lint",
    "test": "pnpm exec nx run-many --target=test",
    "graph": "pnpm exec nx graph",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@changesets/cli": "2.26.2",
    "eslint-config-custom": "workspace:0.0.0",
    "prettier": "latest",
    "turbo": "latest",
    "nx": "17.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@8.10.2"
}
